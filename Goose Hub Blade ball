local function clear_all_plushies()

	if #plushie_temp:GetChildren() == 0 then

		return

	end



	for _, mesh in plushie_temp:GetChildren() do

		Debris:AddItem(mesh, 0)

	end

end



ConnectionsManager[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('uvDYnGJdvhwlXRkyPdEjuqMvIcagcYlICqTEceEqgGHUqTXJmrucfEhcGx1c2hpZQ==')] = RunService.RenderStepped:Connect(function()

    local plushie_enabled = interface.flags[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('xZGhUsdAfYfHnWmibuvxjLcsZxzRgNzexmoaUPEszAPnsiWxDvmaPndcGx1c2hpZQ==')]

    local selected_plushie = interface.flags[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('xdCxqXucxbuWRbfCAsWXajvWoccJNoyeEbNNoqbihWenmhxcbESKuJwcGx1c2hpZV90eXBl')]



    if not plushie_enabled then

        clear_all_plushies()



        return

    end



    if not LocalPlayer.Character or not LocalPlayer.Character.PrimaryPart then

        return

    end



	local protected_name = names_map[selected_plushie]



    if plushie_temp:FindFirstChild(protected_name) then

        local plushie = plushie_temp[protected_name]

        local Root = plushie



