local plushie_temp = Instance.new(mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('ognlMODfAISRoZcHoEzbfHCYrqrECLUxVgKUtSxUiFyuKUzrUjFCdaVRm9sZGVy'), workspace)

