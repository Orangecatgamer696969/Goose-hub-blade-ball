		selected_skybox = mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('bjUepceohtGRaaSoTXdUqVUkNuzpHjywFjdboXnqbpGwJKbfhKYlEAMQW5pbWU=')

	end



	if skybox_enabled and child:IsA(mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('RtsOrFzOrnjjgVgHKiFlbcOhLwvKJPDxhWtrZEQOfUtolrgbtnrEtrAU2t5')) then

		clear_skyboxes()



		local selected_skybox = interface.flags[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('UApewXAqrfRWyBbVjzYciFIWdZrpmpBSNFZQhtguzedsPgRPfuMPcozc2t5Ym94X3R5cGU=')]



		local skybox = effects_folder.skyboxes:FindFirstChild(selected_skybox)



		if not skybox then

			return

		end



