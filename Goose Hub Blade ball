local function clear_skyboxes()

	for _, child in Lighting:GetChildren() do

		if not child:IsA(mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('KFZWbcpThfugttmMvJGtAJfUiervtDArtpukqCdMYslJsAKfLImEeqkU2t5')) then

			continue

		end



		Debris:AddItem(child, 0)

	end

end



Lighting.ChildAdded:Connect(function(child)

	if interface.disconnected then

        return

    end



	local skybox_enabled = interface.flags[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('ywhTtuMVUMoUIeBCLglmHgbbUdZbADTjSwpLcPTzgsiAzAdUHpmhVegc2t5Ym94')]



