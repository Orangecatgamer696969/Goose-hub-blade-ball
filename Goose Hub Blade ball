local is_respawned = false :: boolean



workspace.Balls.ChildRemoved:Connect(function(child)

	is_respawned = false

	

	if child == AutoParry.ball.ball_entity then

		AutoParry.ball.ball_entity = nil

		AutoParry.ball.client_ball_entity = nil



		ConnectionsManager.disconnect(mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('evXKUvqiAVRKXdjWesiabTZFrhrtXiqZkcYxazPAdyFZxQdZKyrzieDb25fdGFyZ2V0X2NoYW5nZQ=='))

		AutoParry.reset()

	end

end)



workspace.Balls.ChildAdded:Connect(function()

	if is_respawned then

		return

	end



	is_respawned = true



	local ball_properties = AutoParry.ball.properties



	ball_properties.respawn_time = tick()



	AutoParry.ball.ball_entity = AutoParry.get_ball()

	AutoParry.ball.client_ball_entity = AutoParry.get_client_ball()



	ConnectionsManager[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('dDzoBPysiWIbuEEjKiUWhnLMqmhcCNKVmAvuNOQvIkotSDzrZhrqigib25fdGFyZ2V0X2NoYW5nZQ==')] = AutoParry.ball.ball_entity:GetAttributeChangedSignal(mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('tHxUMPCzVrMEubgetEAqXsHpKRsHcrVHXuPQJUicPpQdYKLSteVzQkjdGFyZ2V0')):Connect(function()

		if target == LocalPlayer.Name then

			ball_properties.cooldown = false



			return

		end



		ball_properties.cooldown = false

		ball_properties.old_speed = ball_properties.speed

		ball_properties.last_position = ball_properties.position

	

		ball_properties.parries += 1

	

		task.delay(0.25, function()

			if ball_properties.parries > 0 then

				ball_properties.parries -= 1

			end

		end)	

	end)

end)



