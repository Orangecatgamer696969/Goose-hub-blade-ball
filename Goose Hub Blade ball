local dropdown_emotes_table = {}

local emote_instances = {}



for _, emote in ReplicatedStorage.Misc.Emotes:GetChildren() do

	local emote_name = emote:GetAttribute(mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('tNFQMAzrkNXROHwyTNanuMmElXCBINMxxFOIldfRQwmAkcptYhEgWECRW1vdGVOYW1l'))



	if not emote_name then

	return

	end



	table.insert(dropdown_emotes_table, emote_name)

	emote_instances[emote_name] = emote

end



local animation_type = animations.create_dropdown({

	title = mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('bVBvBvYkDliNDviMsngWNyLHKbGgHWBRaUjqRpiLzpHQgDPmkbdhjKQQW5pbWF0aW9u'),

	flag = mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('DStZgOfpFjaDtdnUOmNauXYfxEXDNiAuDNwhZqbdWwMnvgDFpZyNNVIYW5pbWF0aW9uX3R5cGU='),

	mods = dropdown_emotes_table

})



LocalPlayer.Idled:Connect(function()

	VirtualUser:CaptureController()

	VirtualUser:ClickButton2(Vector2.zero)

end)



local current_animation = nil

local current_animation_name = nil



local looped_emotes = {

    mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('oETAMGJPjPySngEzTbLlWZPOoKULranFTxzNOJyNAhXTCCNrRIKomvBRW1vdGUxMDg='),

    mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('wQrCTZufpYENjtPSaaODKiJwViyEsokAWJJsBHcpqKUApacYCeaIMLTRW1vdGUyMjU='),

    mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('HNicOswgWYJxNPgTIOdqMNHvmSIMBAnVObFhSnCIGFmPBidKYafmAtWRW1vdGUzMDA='),

    mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('inCTEyWSFbgehiuOCEiCfWEqDNhVjLsUSTnXarJanIjftMknHPGIBKhRW1vdGUzMDE=')

}



ConnectionsManager[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('sdNvPNOqjqeAIxvYkXEkzgfOmdDLptPUmCCPNgFhThMzjrwuqFYvEtuYW5pbWF0aW9ucw==')] = RunService.Heartbeat:Connect(function()

	local character = LocalPlayer.Character



	if not character then

		if current_animation then

			current_animation:Stop()

			current_animation:Destroy()

		end



		current_animation = nil

		current_animation_name = nil



		return

	end



	local humanoid = character:FindFirstChildOfClass(mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('iKAObXAMNOLnavmMiwCjZhvQxaiiDjRLsxyISuWddztLgDVYkbwzFawSHVtYW5vaWQ='))



	if not humanoid or humanoid.Health <= 0 then

		if current_animation then

			current_animation:Stop()

			current_animation:Destroy()

		end



		current_animation = nil

		current_animation_name = nil



		return

	end



	local animations_enabled = interface.flags[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('TwzLgUtOHcdsKSjOklOGhWmWMSETZRTEBfphIUsrLSOazWLFzuqyRRjYW5pbWF0aW9ucw==')]

	local animation_type = interface.flags[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('ahkkHMIoeQFEZAYlZQDlNdFEMXeivgnSbGayxFEXAvTykyKWooiDjYCYW5pbWF0aW9uX3R5cGU=')]



	if not animations_enabled then

		if current_animation then

			current_animation:Stop()

			current_animation:Destroy()

			current_animation = nil

			current_animation_name = nil

		end



		return

	end



	local smart_animations_enabled = interface.flags[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('RvghUWYMKJaneOmwGpMYPKSmRBsDkRvuDsqcMMoFiuuMKEGUOBgcXSYc21hcnRfYW5pbWF0aW9ucw==')]



	if smart_animations_enabled and Player.Entity.properties.is_moving then

		if current_animation then

			current_animation:Stop()

			current_animation:Destroy()

			current_animation = nil

			current_animation_name = nil

		end

		return

	end



	if not animation_type or current_animation_name == animation_type then

		return

	end



	if current_animation then

		current_animation:Stop()

		current_animation:Destroy()

		current_animation = nil

		current_animation_name = nil

	end



	local animation_object = emote_instances[animation_type]



	if not animation_object then

		return

	end



	local animator = humanoid:FindFirstChildOfClass(mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('RkJGgCdhFTdiGssWjaoWLuBZKqBNKfYqyzvjnXRYmzLSMJsiVhKCIfTQW5pbWF0b3I='))



	if not animator then

		if current_animation then

			current_animation:Stop()

			current_animation:Destroy()

		end	



		current_animation = nil

		current_animation_name = nil



		return

	end



	local animation = Instance.new(mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('AUAinGJTlISNTiTelaFqjmQHndwKdtcOouXavmotUqZvjWYUkbuIPOBQW5pbWF0aW9u'))

	animation.AnimationId = animation_object.AnimationId



	current_animation = animator:LoadAnimation(animation)

	current_animation_name = animation_type



	if not table.find(looped_emotes, animation_object.Name) then

		current_animation.Looped = true

	end



	local time_position = {}



	current_animation:GetMarkerReachedSignal(mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('AmiTVqxaCnGGSuIapfRaHITxRGXTdWUCnnpkjmvhFdEfmwjMJCsuMJyUGlu')):Connect(function(state)

		time_position[state] = current_animation.TimePosition

	end)



	current_animation:GetMarkerReachedSignal(mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('hBbvDFuNBHqimGNlZGVuDYHeGEIxDIxtvlrqKngQcnCfVCcaBGLspgoR09UTw==')):Connect(function(state)

		local current_time_position = time_position[state]



		if current_time_position then

			current_animation.TimePosition = current_time_position

		end

	end)



	ReplicatedStorage.Remotes.CustomEmote:FireServer(true, animation_object.Name)

	current_animation:Play()

end)



ConnectionsManager[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('jQGbtRabJANSpAJEdPGuZbCtipLPuohaIvEdhhbZDpdzUrOxIuzHEoQY2FtZXJhX2ZpZWxkX29mX3ZpZXc=')] = RunService.PostSimulation:Connect(function()

	if not workspace.CurrentCamera then

		return

	end



	local character = LocalPlayer.Character



	if not character then

		return

	end



	local camera_enabled = interface.flags[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('VvUDkHPQXTaXRGkQfhIKqtXiQUPSEsarNLLCgdJVKAMuGCvJnrYmqJuY2FtZXJh')]



	local current_camera = workspace.CurrentCamera



	if not camera_enabled then

		current_camera.FieldOfView = 70



		return

	end



	local field_of_view = interface.flags[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('KfIzrzivPqHfSWeRrOhrHqKKogdtikYRSObhpAumoPwyhtGPAGMDcJrZmllbGRfb2Zfdmlldw==')]



	current_camera.FieldOfView = field_of_view



	if AutoParry.ball.client_ball_entity == nil or #workspace.Balls:GetChildren() == 0 then

		current_camera.CameraSubject = LocalPlayer.Character



		return

	end

end)



local spamming_done = true :: boolean



ConnectionsManager[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('njBLUnyqGynuIzxoWNTEdgmHOpmjfwrNQTNxzYeswaXQYjWGNFeDgvuYWJpbGl0eV92dWxuZXJhYmlsaXR5')] = RunService.PostSimulation:Connect(function()

	local ability_vulnerability_enabled = interface.flags[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('EeBkAAqBbbzCGKcTJHparDeDaoKPzuQoAJzvJxQhYmxXFFCqVkKznoDYWJpbGl0eV92dWxuZXJhYmlsaXR5')]



	if not ability_vulnerability_enabled then

		spamming_done = true

		

		return

	end



	

	local character = LocalPlayer.Character



	if not character then

		return

	end



	if Player.Entity.properties.ping > 250 then

		return

	end



	local mode = interface.flags[mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('YqKQYjtCkymdPsTpolIbwLnEThxDxrCCoKSlZQIemCmnKPlEbazdbjiYWJpbGl0eV92dWxuZXJhYmlsaXR5X21vZGU=')]



	if not spamming_done then

		return

	end



	if not character.Abilities[mode].Enabled then

		return

	end



	if not mode or mode == mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('XhsDavVBVZUnDPEQpUirkYwxBqkutjlUccLJXTioimgGDoHIIpJTiPJUXVhZCBKdW1w') then

		spamming_done = false



		for threads = 1, 3650 do

			ReplicatedStorage.Remotes.XtraJumped:FireServer()

		end



		spamming_done = true

	elseif mode == mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('qwxaQZSowoznAUAnvrSUafhmiqeGiEDfuKdCcUfWCNaAuqcMtWGdJtAQ29udGludWl0eSBaZXJv') and AutoParry.target.current ~= LocalPlayer.Character then

		spamming_done = false



		ReplicatedStorage.Remotes.UseContinuityPortal:FireServer(CFrame.new(tick(), tick(), tick(), tick(), tick(), tick(), tick()))



		task.delay(20, function()

			spamming_done = true

		end)

	elseif mode == mVgGVFVbhuJmuStXusdQoSDkuHwSrlLorvCnEEOhqnQuZStQUeWTz('pDPEiZdVMpIoMQiRefJZRNlDrnsfYpHIAmETTMjugrUgYkMkzhUseRoUXVhc2Fy') then

		spamming_done = false

			

		ReplicatedStorage.Remotes.PlrQuasared:FireServer(AutoParry.target.aim)



		task.delay(0.085, function()

			spamming_done = true

		end)

	end

end)





